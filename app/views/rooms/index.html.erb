<h1>Listing rooms</h1>

<table>
  <tr>
    <th>Roomnum</th>
    <th>Name</th>
    <th>Cname</th>
    <th>Capacity</th>
    <th>Location</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @rooms.each do |room| %>
  <tr>
    <td><%= room.roomnum %></td>
    <td><%= room.name %></td>
    <td><%= room.cname %></td>
    <td><%= room.capacity %></td>
    <td><%= room.location %></td>
    <td><%= link_to 'Details', room %></td>


  </tr>
<% end %>
</table>

